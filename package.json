{
  "name": "light",
  "private": true,
  "version": "0.0.0",
  "main": "dist/electron.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron-build": "electron-builder",
    "dev:ele": "concurrently -k \"BROWSER=none yarn dev\" \"yarn:electron\"",
    "build:ele": "yarn build && yarn electron-build",
    "electron": "wait-on tcp:3000 && electron ."
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.4.4",
    "@mui/material": "^5.4.4",
    "axios": "^0.26.0",
    "electron-is-dev": "^2.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "notistack": "^2.0.3",
    "react": "^17.0.2",
    "react-bootstrap-icons": "^1.7.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6",
    "socket.io-client": "^4.4.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.1.0",
    "electron": "^18.0.3",
    "electron-builder": "^23.0.3",
    "eslint": "^8.10.0",
    "eslint-plugin-react": "^7.29.4",
    "postcss": "^8.4.7",
    "prettier": "^2.6.0",
    "tailwindcss": "^3.0.23",
    "vite": "^2.8.0",
    "wait-on": "^6.0.1"
  },
  "build": {
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "target": "dmg",
      "icon": "./favicon.svg"
    },
    "win": {
      "target": "nsis",
      "icon": "./favicon.svg"
    }
  }
}
